    },
    {
        "atomic layer deposition": "Atomic Layer Deposition"
    },
    {
        " among others. Deposition can be understood to include ": null
    },
    {
        "oxide": "Oxide"
    },
    {
        " layer formation, by ": null
    },
    {
        "thermal oxidation": "Thermal Oxidation"
    },
    {
        " or, more specifically, ": null
    },
    {
        "LOCOS": "Locos"
    },
    {
