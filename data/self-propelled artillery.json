    },
    {
        ", and ": null
    },
    {
        "rocket artillery": "Rocket Artillery"
    },
    {
        ". They are high mobility vehicles, usually based on ": null
    },
    {
        "continuous tracks": "Continuous Track"
    },
    {
        "carrying either a large ": null
    },
    {
        "field gun": "Field Gun"
    },
    {
        ", ": null
    },
    {
        "howitzer": "Howitzer"
    },
    {
        ", ": null
    },
    {
        "mortar": "Mortar"
