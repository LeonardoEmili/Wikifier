    },
    {
        "Rawreth": "Rawreth"
    },
    {
        " in Essex; his father, Thomas, was master of ": null
    },
    {
        "Trinity House": "Trinity House"
    },
    {
        ". Andrewes attended the ": null
    },
    {
        "Coopers free school": "Coopers Company And Coborn School"
    },
    {
        ", in ": null
    },
    {
        "Ratcliff": "Ratcliff"
    },
