    },
    {
        "Avestan": "Avestan"
    },
    {
        " language, used in the ": null
    },
    {
        "Zoroastrian": "Zoroastrian"
    },
    {
        " book the  Avesta .     In 1835, ": null
    },
    {
        "Sir Henry Rawlinson": "Sir Henry Rawlinson, 1St Baronet"
    },
    {
        ", an officer of the ": null
    },
